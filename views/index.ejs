  <%- include('./partials/head') %> 
  <link rel="stylesheet" href="/stylesheets/index.css">
  <body>
    <%- include('./partials/navbar') %> 
    <br>
    <div class="container">
        <div class="card">
            <div id="carouselExampleControlsNoTouching" class="carousel slide" data-bs-touch="false" data-bs-interval="false">
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img src="/images/monitor.svg" class="d-block w-100" alt="/public/images/monitor.svg">
                  </div>
                  <div class="carousel-item">
                    <img src="/images/formulario.svg" class="d-block w-100" alt="/public/images/monitor.svg">
                  </div>
                  <div class="carousel-item">
                    <img src="/images/gameday.svg" class="d-block w-100" alt="/public/images/monitor.svg">
                  </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControlsNoTouching" data-bs-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Anterior</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControlsNoTouching" data-bs-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Próximo</span>
                </button>
              </div>
        </div>

        <br>

        <div class="row">
            <div class="col">
        <div class="card">
            <div class="card-header">
                <h2>Canecas</h2>
            </div>
            <div class="card-body">
                <% for(let i = 0; i < 4; i++) { %> 
                <div class="col-md-4">
                    <img src="/images/gameday.svg" class="img-fluid rounded-start" alt="...">
                  </div>
                  <div class="col-md-8">
                    <div class="card-body">
                      <h5 class="card-title">Card title</h5>
                      <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
                      <p class="card-text"><small class="text-muted">Postado há 5min</small></p>
                      <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                      <button type="button" class="btn btn-link">Ver mais...</button>
                        </div>
                    </div>
                  </div>
                  <hr>
                  <% } %> 
            </div>
        </div>

        <br>

        <div class="card">
            <div class="card-header">
                <h2>Vestuário</h2>
            </div>
